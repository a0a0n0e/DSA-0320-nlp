def fsm_ends_with_ab(string):
    state = 0  
    for char in string:
        if state == 0:
            state = 1 if char == 'a' else 0
        elif state == 1:
            state = 2 if char == 'b' else 1 if char == 'a' else 0
    return state == 2 
test_strings = ["ab", "aab", "xyzab", "abc"]
for s in test_strings:
    print(f"'{s}' ends with 'ab': {fsm_ends_with_ab(s)}")
